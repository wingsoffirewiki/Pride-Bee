{
  "name": "pride-bee",
  "version": "2.0.0",
  "description": "A bot for adding flags to your discord avatar!",
  "main": "dist/index.js",
  "author": "wofwiki <wings.of.fire.fandom.wiki@gmail.com>",
  "license": "MIT",
  "scripts": {
    "run": "node --enable-source-maps .",
    "start": "yarn build && yarn run run",
    "dev": "nodemon",
    "build": "tsc",
