    "lint-fix": "eslint --fix -c .eslintrc.json \"src/**/*.ts\""
  },
  "dependencies": {
    "axios": "0.27.2",
    "discord.js": "13.8.1",
    "fero-dc": "4.0.0-dev-2",
    "sharp": "0.30.7"
  },
  "devDependencies": {
    "@types/sharp": "0.30.4",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "dotenv": "16.0.1",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
